<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sds.webapp.acc.dao.MerchantDAO">
	<select id="findByKey" parameterType="MerchantDomain"
		resultType="MerchantDomain" useCache="true">
		select
		ID,ACCOUNT,PASSWORD,PRIVATEKEY,CREATE_DATE,REAL_NAME,CMER,CMER_SORT,CHANNEL_CODE,BUSINESS_ID,CARD_NO,CERT_NO,PHONE,MOBILE,LOCATION,CERT_CORRECT,CERT_OPPOSITE,CERT_MEET,CARD_CORRECT,CARD_OPPOSITE,STATUS,OTHER,WX_RATE,ALI_RATE,APP_CODE,TJ_ID,USER_TYPE,BUS_PIC,BUS_NO,AGENT_ID
		from fm_merchant
		<where>
			<if test="id !=null">
				and ID = #{id}
			</if>
		</where>
	</select>
	<select id="findByWhere" parameterType="MerchantDomain"
		resultType="MerchantDomain" useCache="true">
