			</if>
			<if test="certNo !=null">
				and CERT_NO = #{certNo}
			</if>
			<if test="phone !=null">
				and PHONE = #{phone}
			</if>
			<if test="mobile !=null">
				and MOBILE = #{mobile}
			</if>
			<if test="location !=null">
				and LOCATION = #{location}
			</if>
			<if test="certCorrect !=null">
				and CERT_CORRECT = #{certCorrect}
			</if>
			<if test="certOpposite !=null">
				and CERT_OPPOSITE = #{certOpposite}
			</if>
			<if test="certMeet !=null">
				and CERT_MEET = #{certMeet}
			</if>
			<if test="cardCorrect !=null">
				and CARD_CORRECT = #{cardCorrect}
			</if>
			<if test="cardOpposite !=null">
				and CARD_OPPOSITE = #{cardOpposite}
			</if>
			<if test="status !=null">
				and STATUS = #{status}
			</if>
			<if test="other !=null">
				and OTHER = #{other}
			</if>
			<if test="wxRate !=null">
				and WX_RATE = #{wxRate}
			</if>
			<if test="aliRate !=null">
				and ALI_RATE = #{aliRate}
			</if>
			<if test="appCode !=null">
				and APP_CODE = #{appCode}
			</if>
			<if test="tjId !=null">
				and TJ_ID = #{tjId}
			</if>
			<if test="userType !=null">
				and USER_TYPE = #{userType}
			</if>
			<if test="busPic !=null">
				and BUS_PIC = #{busPic}
			</if>
			<if test="busNo !=null">
				and BUS_NO = #{busNo}
			</if>
			<if test="agentId !=null">
				and AGENT_ID = #{agentId}
			</if>
		</where>
	</select>
	<insert id="insert" parameterType="MerchantDomain" flushCache="true">
		insert into
		fm_merchant(ID,ACCOUNT,PASSWORD,PRIVATEKEY,CREATE_DATE,REAL_NAME,CMER,CMER_SORT,CHANNEL_CODE,BUSINESS_ID,CARD_NO,CERT_NO,PHONE,MOBILE,LOCATION,CERT_CORRECT,CERT_OPPOSITE,CERT_MEET,CARD_CORRECT,CARD_OPPOSITE,STATUS,OTHER,WX_RATE,ALI_RATE,APP_CODE,TJ_ID,USER_TYPE,BUS_PIC,BUS_NO,AGENT_ID)
		values
		(#{id},#{account},#{password},#{privatekey},#{createDate},#{realName},#{cmer},#{cmerSort},#{channelCode},#{businessId},#{cardNo},#{certNo},#{phone},#{mobile},#{location},#{certCorrect},#{certOpposite},#{certMeet},#{cardCorrect},#{cardOpposite},#{status},#{other},#{wxRate},#{aliRate},#{appCode},#{tjId},#{userType},#{busPic},#{busNo},#{agentId})
	</insert>
