		ID,ACCOUNT,PRIVATEKEY,CREATE_DATE,REAL_NAME,CMER,CMER_SORT,CHANNEL_CODE,BUSINESS_ID,CARD_NO,CERT_NO,PHONE,MOBILE,LOCATION,CERT_CORRECT,CERT_OPPOSITE,CERT_MEET,CARD_CORRECT,CARD_OPPOSITE,STATUS,OTHER,WX_RATE,ALI_RATE,APP_CODE,TJ_ID,USER_TYPE,BUS_PIC,BUS_NO,AGENT_ID
		from fm_merchant
		<where>
			<if test="id !=null">
				and ID = #{id}
			</if>
			<if test="account !=null">
				and ACCOUNT = #{account}
			</if>
			<if test="password !=null">
				and PASSWORD = #{password}
			</if>
			<if test="privatekey !=null">
				and PRIVATEKEY = #{privatekey}
			</if>
			<if test="createDate !=null">
				and CREATE_DATE = #{createDate}
			</if>
			<if test="realName !=null">
				and REAL_NAME = #{realName}
			</if>
			<if test="cmer !=null">
				and CMER = #{cmer}
			</if>
			<if test="cmerSort !=null">
				and CMER_SORT = #{cmerSort}
			</if>
			<if test="channelCode !=null">
				and CHANNEL_CODE = #{channelCode}
			</if>
			<if test="businessId !=null">
				and BUSINESS_ID = #{businessId}
			</if>
			<if test="cardNo !=null">
